@{
    Layout = null;
}
<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>DALK ANALYZER</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta content="A fully featured admin theme which can be used to build CRM, CMS, etc." name="description">
    <meta content="Coderthemes" name="author">
    <link rel="shortcut icon" href="~/Content/images/favicon.ico">
    <style>
        body.loading {
            visibility: hidden
        }
    </style>
    <link rel="shortcut icon" href="~/Content/images/favicon.ico">
    <link href="~/Content/css/app.min.css" rel="stylesheet">
    <link href="~/Content/css/icons.css" rel="stylesheet">
</head>
<body class="loading">
    <div class="wrapper">
        @Html.Partial("_LeftSideBar")
        <div class="content-page">
            <div class="content">
                <div class="navbar-custom">
                    <ul class="list-unstyled topbar-right-menu float-right mb-0">
                        <li class="notification-list topbar-dropdown d-lg-block"></li>
                    </ul>
                    <button class="button-menu-mobile open-left disable-btn"><i class="mdi mdi-menu" id="button-menu-mobile-toggle"></i></button>
                    <div class="app-search">
                        <h4>Stronka startowa</h4>
                    </div>
                </div>
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-12">
                            <div class="page-title-box">
                                <div class="page-title-right">
                                </div>
                                <h4 class="page-title">Zaczynamy!</h4>
                            </div>
                        </div>
                    </div>
                    <div class="card m-b-30">
                        <div class="card-body">
                            <div class="p-lg-3">
                                Strona startowa DALK ANALYZER!
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <footer class="footer">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-6">2019 © Hyper - Coderthemes.com</div>
                    <div class="col-md-6">
                        <div class="text-md-right footer-links d-none d-md-block">
                            <a href="https://coderthemes.com/" target="_blank">About</a>
                            <a href="https://coderthemes.com/" target="_blank">Support</a>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </div>
    <script src="~/Content/js/app.min.js"></script>
</body>
</html>